<h1>Score</h1>

<table>
  <tr>
    <th>Score</th>
  </tr>

<% @scores.each do |score| %>
  <tr>
    <td><%=score.number %></td>
  </tr>
<% end %>
</table>

<br>
<button class="button button1"><%= link_to "New Score", new_score_path %></button>
<button class="button button1"><%= link_to "back", articles_path %></button>

  