<h1>Albums</h1>

<table>
  <tr>
    <th>Album</th>
    <th>Score</th>
  </tr>

<% @albums.each do |album| %>
  <tr>
    <td><%= link_to album.title, album %></td>
    <td><%=album.score.number %></td>
  </tr>
<% end %>
</table>
<br>
<button class="button button1"><%= link_to "New Album", new_album_path %></button>
<br>
<button class="button button1"><%= link_to "back", articles_path %></button>